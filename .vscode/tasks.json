{
	"tasks": [
		{
			"type": "cargo",
			"subcommand": "build",
			"args": [
				"--example=${fileBasenameNoExtension}"
			],
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"problemMatcher": [
				"$rustc"
			],
			"group": "build",
			"label": "Build current example"
		}
	],
	"version": "2.0.0"
}
